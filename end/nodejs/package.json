{
  "name": "api-proxy-nodejs",
  "version": "1.0.0",
  "description": "OAuth2 Mock Service for OpenResty AI Proxy",
  "main": "server/app.js",
  "scripts": {
    "start": "node server/app.js",
    "dev": "nodemon server/app.js",
    "build": "npm run build:client",
    "build:client": "cd client && npm run build",
    "install:client": "cd client && npm install",
    "pm2:start": "pm2 start pm2.config.js",
    "pm2:stop": "pm2 stop oauth2-mock",
    "pm2:restart": "pm2 restart oauth2-mock",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "node server/tests/unit/",
    "test:integration": "node server/tests/integration/"
  },
  "keywords": [
    "oauth2",
    "mock",
    "nodejs",
    "express",
    "openresty",
    "ai-proxy"
  ],
  "author": "API Proxy Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "node-forge": "^1.3.1",
    "redis": "^4.6.12",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "pm2": "^6.0.14",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "local"
  },
  "bugs": {
    "url": "local"
  },
  "homepage": "local"
}
